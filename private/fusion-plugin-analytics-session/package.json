{
  "name": "@uber/fusion-plugin-analytics-session",
  "description": "Plugin for generating and providing analytics sessions in the cookie",
  "version": "1.2.1",
  "author": "Web Platform Team <eng-web-platform@uber.com>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "gitolite@code.uber.internal:web/fusion-plugin-analytics-session.git"
  },
  "bugs": {
    "url": "https://code.uberinternal.com/maniphest/task/create/",
    "email": "eng-web-platform@uber.com"
  },
  "files": [
    "dist",
    "src",
    "!__tests__"
  ],
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn",
    "lint": "eslint src --fix",
    "flow": "flow check || true",
    "build": "cup build && eslint src",
    "dev": "npm-watch | grep --invert-match nodemon",
    "test": "jest && yarn flow",
    "cover": "yarn lint && yarn flow && yarn test",
    "prepublish": "cup build && eslint src"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "quiet": true
    },
    "test": {
      "patterns": [
        "src"
      ],
      "quiet": true
    }
  },
  "dependencies": {
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "create-universal-package": "^3.4.6",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.8.0",
    "eslint-config-fusion": "^4.0.0",
    "eslint-plugin-cup": "^2.0.0",
    "eslint-plugin-flowtype": "^3.1.4",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.86.0",
    "fusion-core": "^1.9.1-0",
    "fusion-test-utils": "^1.2.3",
    "fusion-tokens": "^1.1.0",
    "jest": "^23.6.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "npm-watch": "^0.4.0",
    "prettier": "^1.14.3"
  },
  "peerDependencies": {
    "fusion-core": "^1.9.1-0",
    "fusion-tokens": "^1.1.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "browser": {
    "./dist/index.js": "./dist/browser.es5.js",
    "./dist/index.es.js": "./dist/browser.es5.es.js"
  },
  "es2015": {
    "./dist/browser.es5.es.js": "./dist/browser.es2015.es.js"
  },
  "es2017": {
    "./dist/browser.es5.es.js": "./dist/browser.es2017.es.js",
    "./dist/browser.es2015.es.js": "./dist/browser.es2017.es.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-flow",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/type.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "cobertura",
      "html",
      "json",
      "text-summary",
      "text"
    ],
    "projects": [
      {
        "displayName": "browser",
        "browser": true,
        "testEnvironment": "jest-environment-jsdom-global",
        "testPathIgnorePatterns": [
          "/node_modules/",
          "/dist/",
          ".*\\.node\\.js"
        ],
        "testURL": "http://localhost",
        "globals": {
          "__DEV__": true,
          "__NODE__": false,
          "__BROWSER__": true
        }
      },
      {
        "displayName": "server",
        "browser": false,
        "testEnvironment": "node",
        "testPathIgnorePatterns": [
          "/node_modules/",
          "/dist/",
          ".*\\.browser\\.js"
        ],
        "globals": {
          "__DEV__": true,
          "__NODE__": true,
          "__BROWSER__": false
        }
      }
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-fusion"
    ]
  },
  "engines": {
    "node": ">=8.3.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "private": true
}
