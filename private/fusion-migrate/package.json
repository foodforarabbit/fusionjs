{
  "name": "@uber/fusion-migrate",
  "version": "0.1.0",
  "description":
    "Migration scripts for migrating from @uber/bedrock to Fusion.js",
  "bin": {
    "ufusion": "bin/fusion-migrate.js"
  },
  "scripts": {
    "test": "jest",
    "cover": "jest --collectCoverage",
    "integration": "node src/integration-test.js",
    "debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@uber/uber-web-remote-template": "^1.7.0",
    "args": "^3.0.8",
    "babel-core": "^6.26.0",
    "babel-generator": "^6.26.1",
    "babel-types": "^6.26.0",
    "babylon": "^6.18.0",
    "chalk": "^2.3.2",
    "execa": "^0.10.0",
    "inquirer": "^5.2.0",
    "just-safe-get": "^1.2.1",
    "ncp": "^2.0.0",
    "semver": "^5.5.0",
    "tmp": "^0.0.33"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-tester": "^5.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "create-universal-package": "^3.4.0",
    "eslint": "^4.17.0",
    "eslint-config-fusion": "^1.0.0",
    "eslint-plugin-cup": "^1.0.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "flow-bin": "^0.66.0",
    "jest": "^22.4.2",
    "prettier": "1.11.1"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "jest": {
    "coverageReporters": ["text", "cobertura"],
    "collectCoverageFrom": ["src/**/*.js"],
    "coveragePathIgnorePatterns": ["/node_modules/", "/__fixtures__/"]
  },
  "repository": "https://code.uberinternal.com/diffusion/WEFUSGK/"
}
