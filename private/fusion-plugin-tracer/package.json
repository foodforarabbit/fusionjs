{
  "name": "@uber/fusion-plugin-tracer",
  "description": "A fusion plugin for jaeger tracing",
  "version": "1.0.6",
  "author": "Web Platform Team <eng-web-platform@uber.com>",
  "license": "UNLICENSED",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "browser": {
    "./dist/index.js": "./dist/browser.es5.js",
    "./dist/index.es.js": "./dist/browser.es5.es.js"
  },
  "es2015": {
    "./dist/browser.es5.es.js": "./dist/browser.es2015.es.js"
  },
  "es2017": {
    "./dist/browser.es5.es.js": "./dist/browser.es2017.es.js",
    "./dist/browser.es2015.es.js": "./dist/browser.es2017.es.js"
  },
  "dependencies": {
    "@uber/jaeger-client-adapter": "^3.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "create-universal-package": "^3.4.6",
    "eslint": "^5.16.0",
    "eslint-config-fusion": "^4.0.0",
    "eslint-plugin-cup": "^2.0.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.95.1",
    "fusion-core": "^1.10.4-1",
    "fusion-test-utils": "^1.4.1-0",
    "fusion-tokens": "^1.1.1",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "tape-cup": "^4.7.1",
    "unitest": "^2.1.1"
  },
  "peerDependencies": {
    "fusion-core": "^1.10.4-1",
    "fusion-tokens": "^1.1.1"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "flow": "flow check",
    "lint": "eslint .",
    "build-test": "rm -rf dist-tests && cup build-tests",
    "just-test": "node_modules/.bin/unitest --browser=dist-tests/browser.js NODE_ENV=test --node=dist-tests/node.js",
    "cover": "npm run build-test && npm run just-cover",
    "just-cover": "nyc --reporter=cobertura npm run just-test",
    "view-cover": "npm run build-test && nyc --reporter=html npm run just-test && open coverage/index.html",
    "test": "npm run build-test && npm run just-test",
    "prepublish": "npm run transpile",
    "build": "npm run clean && cup build"
  },
  "engines": {
    "node": ">=8.3.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "private": true
}