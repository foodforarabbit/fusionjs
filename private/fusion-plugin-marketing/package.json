{
  "name": "@uber/fusion-plugin-marketing",
  "description": "Fusion.js plugin for web marketing attributions.",
  "version": "0.0.0-monorepo",
  "scaffold-version": "0.0.19",
  "author": " <dennis.lin@uber.com>",
  "repository": {
    "directory": "private/fusion-plugin-marketing",
    "type": "git",
    "url": "https://github.com/uber/fusionjs"
  },
  "bugs": {
    "url": "https://code.uberinternal.com/maniphest/task/create/",
    "email": "dennis.lin@uber.com"
  },
  "license": "UNLICENSED",
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn",
    "lint": "eslint src",
    "flow": "flow check",
    "build": "cup build",
    "dev": "npm-watch | grep --invert-match nodemon",
    "test": "jest",
    "cover": "yarn lint && yarn flow && yarn test",
    "clean": "rm -rf dist",
    "prepublish": "npm run build"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "quiet": true
    },
    "test": {
      "patterns": [
        "src"
      ],
      "quiet": true
    }
  },
  "dependencies": {
    "fast-safe-stringify": "^2.0.6",
    "isbot": "^2.2.0",
    "parse-domain": "^2.1.2",
    "request-ip": "^2.1.1",
    "uuid": "^3.3.2",
    "validator": "^10.7.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@uber/fusion-plugin-heatpipe": "0.0.0-monorepo",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "create-universal-package": "^3.4.7",
    "eslint": "^6.0.1",
    "eslint-config-fusion": "0.0.0-monorepo",
    "eslint-plugin-cup": "^2.0.1",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.2",
    "flow-bin": "^0.102.0",
    "fusion-core": "0.0.0-monorepo",
    "fusion-test-utils": "0.0.0-monorepo",
    "fusion-tokens": "0.0.0-monorepo",
    "jest": "^24.8.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "koa": "^2.7.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.18.2",
    "eslint-plugin-react-hooks": "^1.6.1"
  },
  "peerDependencies": {
    "@uber/fusion-plugin-heatpipe": "0.0.0-monorepo",
    "fusion-core": "0.0.0-monorepo",
    "fusion-tokens": "0.0.0-monorepo"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "browser": {
    "./dist/index.js": "./dist/browser.es5.js",
    "./dist/index.es.js": "./dist/browser.es5.es.js"
  },
  "es2015": {
    "./dist/browser.es5.es.js": "./dist/browser.es2015.es.js"
  },
  "es2017": {
    "./dist/browser.es5.es.js": "./dist/browser.es2017.es.js",
    "./dist/browser.es2015.es.js": "./dist/browser.es2017.es.js"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-flow"
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-runtime"
        ]
      }
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/type.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "cobertura",
      "html",
      "json",
      "text-summary",
      "text"
    ],
    "projects": [
      {
        "displayName": "browser",
        "browser": true,
        "testEnvironment": "jest-environment-jsdom-global",
        "testPathIgnorePatterns": [
          "/node_modules/",
          "/dist/",
          ".*\\.node\\.js"
        ],
        "testURL": "http://localhost",
        "globals": {
          "__DEV__": true,
          "__NODE__": false,
          "__BROWSER__": true
        }
      },
      {
        "displayName": "server",
        "browser": false,
        "testEnvironment": "node",
        "testPathIgnorePatterns": [
          "/node_modules/",
          "/dist/",
          ".*\\.browser\\.js"
        ],
        "globals": {
          "__DEV__": true,
          "__NODE__": true,
          "__BROWSER__": false
        }
      }
    ]
  },
  "engines": {
    "node": ">=8.9.0 <11",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "files": [
    "dist",
    "src",
    "!__tests__"
  ],
  "homepage": "https://engdocs.uberinternal.com/web/api/fusion-plugin-marketing",
  "sideEffects": false
}
