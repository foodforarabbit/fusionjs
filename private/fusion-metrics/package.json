{
  "name": "@uber/fusion-metrics",
  "description": "Metrics tracking for Fusion apps",
  "version": "0.0.0-monorepo",
  "scaffold-version": "0.2.5",
  "author": " <lhorie@uber.com>",
  "repository": {
    "directory": "private/fusion-metrics",
    "type": "git",
    "url": "https://github.com/uber/fusionjs"
  },
  "bugs": {
    "url": "https://code.uberinternal.com/maniphest/task/create/",
    "email": "lhorie@uber.com"
  },
  "license": "UNLICENSED",
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn",
    "lint": "eslint src",
    "flow": "flow",
    "build": "cup-build",
    "dev": "npm-watch | grep --invert-match nodemon",
    "test": "jest",
    "prepublish": "cup-build"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "quiet": true
    },
    "test": {
      "patterns": [
        "src"
      ],
      "quiet": true
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "create-universal-package": "^4.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.0.1",
    "eslint-config-fusion": "0.0.0-monorepo",
    "eslint-plugin-cup": "^2.0.1",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "flow-bin": "^0.109.0",
    "fusion-plugin-introspect": "0.0.0-monorepo",
    "jest": "^24.9.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.18.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "peerDependencies": {
    "fusion-plugin-introspect": "0.0.0-monorepo",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "main": "./dist-node-cjs/index.js",
  "module": "./dist-node-esm/index.js",
  "browser": {
    "./dist-node-cjs/index.js": "./dist-browser-cjs/index.js",
    "./dist-node-esm/index.js": "./dist-browser-esm/index.js"
  },
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "files": [
    "dist-*/",
    "src/",
    "!__tests__"
  ],
  "resolutions": {
    "event-stream": "4.0.1"
  },
  "homepage": "https://engdocs.uberinternal.com/web/api/fusion-metrics",
  "sideEffects": false
}
