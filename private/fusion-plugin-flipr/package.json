{
  "name": "@uber/fusion-plugin-flipr",
  "description": "Flipr client plugin for fusion",
  "version": "0.1.0",
  "author": "Web Platform Team <eng-web-platform@uber.com>",
  "license": "UNLICENSED",
  "main": "./dist/node.cjs.js",
  "module": "./dist/node.es.js",
  "browser": {
    "./dist/node.cjs.js": "./dist/browser.cjs.js",
    "./dist/node.es.js": "./dist/browser.es.js"
  },
  "es2015": {
    "./dist/node.cjs.js": "./dist/node.cjs.es2015.js",
    "./dist/node.es.js": "./dist/node.es.es2015.js",
    "./dist/browser.cjs.js": "./dist/browser.cjs.es2015.js",
    "./dist/browser.es.js": "./dist/browser.es.es2015.js"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-plugin-istanbul": "^4.1.5",
    "create-universal-package": "^1.0.0-rc.14",
    "eslint": "^4.2.0",
    "eslint-config-fusion": "^0.1.0",
    "eslint-plugin-cup": "^1.0.0-rc.4",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "fusion-plugin": "^0.1.1",
    "nyc": "^11.2.1",
    "prettier": "1.4.2",
    "tape-cup": "^4.7.1",
    "unitest": "^1.1.0"
  },
  "peerDependencies": {
    "fusion-plugin": "^0.1.1"
  },
  "scripts": {
    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "transpile": "npm run clean && cup build",
    "build-test": "rm -rf dist-tests && NODE_ENV=test cup build-tests",
    "just-test": "unitest --browser=dist-tests/browser.js --node=dist-tests/node.js",
    "test": "npm run lint && npm run build-test && npm run just-test",
    "cover": "npm run build-test && npm run just-cover",
    "just-cover": "nyc --reporter=cobertura npm run just-test",
    "view-cover": "npm run build-test && nyc --reporter=html npm run just-test && open coverage/index.html",
    "prepublish": "npm run transpile"
  },
  "dependencies": {
    "@uber/flipr-client": "^17.0.10"
  },
  "engines": {
    "node": ">=8.6.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  }
}
