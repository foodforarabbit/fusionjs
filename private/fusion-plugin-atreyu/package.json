    "fusion-core": "^1.10.0",
    "fusion-tokens": "^1.1.1"
  },
  "devDependencies": {
    "@uber/fusion-plugin-galileo": "^1.0.4",
    "@uber/fusion-plugin-m3": "^1.1.6",
