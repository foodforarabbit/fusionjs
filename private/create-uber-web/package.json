{
  "name": "@uber/create-uber-web",
  "version": "2.0.8",
  "description": "CLI to scaffold a new Uber web application or library",
  "repository": {
    "type": "git",
    "url": "gitolite@code.uber.internal:web/uber-web.git"
  },
  "bin": {
    "@uber/create-uber-web": "dist/cli.js",
    "create-uber-web": "dist/cli.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm-watch | grep --invert-match nodemon",
    "build": "babel src -d dist -q",
    "lint": "eslint src",
    "test": "$(find templates -name node_modules -prune -exec rm -rf {} \\;); jest --maxWorkers 1",
    "cover": "jest --coverage",
    "flow": "flow check",
    "prepublish": "babel src -d dist -s"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "quiet": true
    },
    "test": {
      "patterns": [
        "src"
      ],
      "quiet": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/types": "^7.4.0",
    "@dubstep/core": "^3.3.1",
    "@ganemone/babel-flow-types": "^2.0.6",
    "@uber/awd-client": "^0.5.0",
    "chalk": "^2.4.2",
    "cli-table3": "^0.5.1",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.2.2",
    "js-yaml": "^3.13.0",
    "just-safe-get": "^1.3.0",
    "log-update": "^3.2.0",
    "nunjucks": "^3.2.0",
    "sade": "^1.4.2",
    "semver": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.16.0",
    "eslint-config-fusion": "^5.0.0",
    "eslint-plugin-cup": "^2.0.1",
    "eslint-plugin-flowtype": "^3.5.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "flow-bin": "^0.96.0",
    "jest": "^24.7.1",
    "npm-watch": "^0.6.0",
    "prettier": "^1.16.4"
  },
  "resolutions": {
    "event-stream": "3.3.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-flow"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "jest": {
    "testRegex": "src/(commands|utils|codemods)/.*?.test.js",
    "coverageReporters": [
      "text",
      "cobertura"
    ]
  },
  "engines": {
    "node": ">=8.9.4 <11",
    "npm": ">=5.6.0",
    "yarn": ">=1.4.0"
  },
  "private": true
}
