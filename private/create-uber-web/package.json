{
  "name": "@uber/create-uber-web",
  "version": "1.1.12",
  "description": "CLI to scaffold a new Uber web application or library",
  "repository": {
    "type": "git",
    "url": "gitolite@code.uber.internal:web/uber-web.git"
  },
  "bin": {
    "@uber/create-uber-web": "dist/cli.js",
    "create-uber-web": "dist/cli.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm-watch | grep --invert-match nodemon",
    "build": "babel src -d dist -q",
    "lint": "eslint src",
    "test": "ussh && jest",
    "cover": "jest --coverage",
    "flow": "flow check",
    "prepublish": "babel src -d dist -s"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "quiet": true
    },
    "test": {
      "patterns": [
        "src"
      ],
      "quiet": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/types": "^7.3.3",
    "@dubstep/core": "^2.0.4",
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.2.2",
    "log-update": "^2.3.0",
    "nunjucks": "^3.1.7",
    "sade": "^1.4.2",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.14.1",
    "eslint-config-fusion": "^4.0.0",
    "eslint-plugin-cup": "^2.0.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.92.1",
    "jest": "^24.1.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.16.4"
  },
  "resolutions": {
    "event-stream": "3.3.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-flow"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "jest": {
    "testRegex": "src/(commands|utils|codemods)/.*?.test.js",
    "coverageReporters": [
      "text",
      "cobertura"
    ]
  },
  "engines": {
    "node": ">=8.9.4 <9.0.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.4.0"
  },
  "private": true
}
