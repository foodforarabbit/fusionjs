{
  "name": "@uber/create-uber-web",
  "description": "CLI to scaffold a new Uber web application or library",
  "version": "0.0.0-monorepo",
  "bin": "dist/cli.js",
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/types": "^7.5.0",
    "@dubstep/core": "^3.5.2",
    "@ganemone/babel-flow-types": "^2.0.7",
    "@uber-web-ui/baseui-codemods": "^1.0.9",
    "@uber/awd-client": "^1.1.1",
    "chalk": "^2.4.2",
    "cli-table3": "^0.5.1",
    "flow-typed": "^2.6.1",
    "fs-extra": "^7.0.1",
    "inquirer": "^7.0.4",
    "js-yaml": "^3.13.0",
    "just-safe-get": "^1.3.0",
    "log-update": "^3.2.0",
    "nunjucks": "^3.2.0",
    "sade": "^1.6.0",
    "semver": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-flow-strip-types": "^7.12.1",
    "@babel/preset-env": "^7.8.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-fusion": "0.0.0-monorepo",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-cup": "^2.0.2",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.1.0",
    "flow-bin": "^0.109.0",
    "jazelle": "latest",
    "jest": "^25.1.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.19.1"
  },
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "files": [
    "dist/",
    "src/codemods/inline-react-hocs/templates/",
    "templates/"
  ],
  "homepage": "https://engdocs.uberinternal.com/web/api/create-uber-web",
  "main": "dist/index.js",
  "repository": {
    "directory": "private/create-uber-web",
    "type": "git",
    "url": "https://github.com/uber/fusionjs"
  },
  "resolutions": {
    "event-stream": "3.3.5"
  },
  "scripts": {
    "prepack": "babel src -d dist && yarn workspaces foreach -A --include='@uber/template-*' run prepack && ./prepublish.sh",
    "cover": "yarn run test --coverage",
    "dev": "npm-watch | grep --invert-match nodemon",
    "flow": "flow",
    "lint": "eslint src",
    "prepublish": "babel src -d dist && ./prepublish.sh",
    "test": "jest --maxWorkers 1"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "quiet": true
    },
    "test": {
      "patterns": [
        "src"
      ],
      "quiet": true
    }
  }
}
