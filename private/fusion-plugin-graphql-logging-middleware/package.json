{
  "name": "@uber/fusion-plugin-graphql-logging-middleware",
  "description": "A fusion plugin which provides a graphql middleware to handle logging, tracing, and metrics",
  "version": "0.0.0-monorepo",
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn",
    "lint": "eslint src --fix",
    "flow": "flow",
    "build": "cup-build",
    "dev": "npm-watch | grep --invert-match nodemon",
    "test": "jest",
    "prepublish": "cup-build"
  },
  "dependencies": {
    "graphql-middleware": "^3.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@uber/fusion-plugin-m3": "0.0.0-monorepo",
    "@uber/fusion-plugin-tracer": "0.0.0-monorepo",
    "babel-eslint": "^10.0.3",
    "create-universal-package": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-fusion": "0.0.0-monorepo",
    "eslint-plugin-cup": "^2.0.2",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "flow-bin": "^0.109.0",
    "fusion-core": "0.0.0-monorepo",
    "fusion-plugin-apollo": "0.0.0-monorepo",
    "fusion-plugin-universal-events": "0.0.0-monorepo",
    "fusion-react": "0.0.0-monorepo",
    "fusion-test-utils": "0.0.0-monorepo",
    "fusion-tokens": "0.0.0-monorepo",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "jest": "^25.1.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.19.1",
    "react-apollo": "^2.5.8"
  },
  "peerDependencies": {
    "@uber/fusion-plugin-m3": "0.0.0-monorepo",
    "@uber/fusion-plugin-tracer": "0.0.0-monorepo",
    "fusion-core": "0.0.0-monorepo",
    "fusion-plugin-apollo": "0.0.0-monorepo",
    "fusion-tokens": "0.0.0-monorepo",
    "graphql": "^14.5.8"
  },
  "files": [
    "dist-*/",
    "src/",
    "!__tests__"
  ],
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.6.0",
    "yarn": ">=1.4.0"
  },
  "main": "./dist-node-cjs/index.js",
  "module": "./dist-node-esm/index.js",
  "browser": {
    "./dist-node-cjs/index.js": "./dist-browser-cjs/index.js",
    "./dist-node-esm/index.js": "./dist-browser-esm/index.js"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "quiet": true
    },
    "test": {
      "patterns": [
        "src"
      ],
      "quiet": true
    }
  },
  "license": "UNLICENSED",
  "sideEffects": false
}
