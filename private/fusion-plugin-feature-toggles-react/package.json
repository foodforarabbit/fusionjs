{
  "name": "@uber/fusion-plugin-feature-toggles-react",
  "description": "The &#39;fusion-plugin-feature-toggles-react&#39; package provides a universal feature toggles plugin for Fusion.js",
  "version": "0.0.1",
  "scaffold-version": "1.1.10",
  "author": " <amsmith@uber.com>",
  "repository": {
    "type": "git",
    "url": "gitolite@code.uber.internal:web/fusion-plugin-feature-toggles-react.git"
  },
  "bugs": {
    "url": "https://code.uberinternal.com/maniphest/task/create/",
    "email": "amsmith@uber.com"
  },
  "license": "UNLICENSED",
  "scripts": {
    "bootstrap": "rm -rf node_modules && yarn",
    "lint": "eslint src --fix",
    "flow": "flow check || true",
    "build": "cup build && eslint src",
    "dev": "npm-watch | grep --invert-match nodemon",
    "test": "jest && yarn flow",
    "cover": "yarn lint && yarn flow && yarn test",
    "prepublish": "cup build && eslint src"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "quiet": true
    },
    "test": {
      "patterns": [
        "src"
      ],
      "quiet": true
    }
  },
  "dependencies": {
    "@uber/fusion-plugin-feature-toggles": "0.1.1-1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/preset-env": "7.4.2",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@uber/atreyu": "^6.1.1",
    "@uber/fusion-plugin-atreyu": "^1.3.1",
    "@uber/fusion-plugin-galileo": "^1.0.5",
    "@uber/fusion-plugin-m3": "^1.1.10",
    "@uber/fusion-plugin-tchannel": "^1.1.1",
    "@uber/fusion-plugin-tracer": "^1.0.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "create-universal-package": "3.4.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.3",
    "eslint-config-fusion": "^4.0.0",
    "eslint-plugin-cup": "^2.0.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.95.1",
    "fusion-core": "^1.10.6",
    "fusion-plugin-universal-events": "^1.3.3",
    "fusion-react": "^2.0.0",
    "fusion-test-utils": "^1.4.1",
    "fusion-tokens": "^1.1.1",
    "jest": "^24.5.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.16.4"
  },
  "peerDependencies": {
    "fusion-core": "^1.10.3",
    "fusion-react": "^1.3.7",
    "fusion-tokens": "^1.1.1",
    "react": "14.x - 16.x"
  },
  "resolutions": {
    "event-stream": "3.3.5"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "browser": {
    "./dist/index.js": "./dist/browser.es5.js",
    "./dist/index.es.js": "./dist/browser.es5.es.js"
  },
  "es2015": {
    "./dist/browser.es5.es.js": "./dist/browser.es2015.es.js"
  },
  "es2017": {
    "./dist/browser.es5.es.js": "./dist/browser.es2017.es.js",
    "./dist/browser.es2015.es.js": "./dist/browser.es2017.es.js"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-flow",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/type.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "cobertura",
      "html",
      "json",
      "text-summary",
      "text"
    ],
    "projects": [
      {
        "displayName": "browser",
        "browser": true,
        "testEnvironment": "jest-environment-jsdom-global",
        "testPathIgnorePatterns": [
          "/node_modules/",
          "/dist/",
          ".*\\.node\\.js"
        ],
        "testURL": "http://localhost",
        "setupTestFrameworkScriptFile": "<rootDir>enzyme.config.js",
        "globals": {
          "__DEV__": true,
          "__NODE__": false,
          "__BROWSER__": true
        }
      },
      {
        "displayName": "server",
        "browser": false,
        "testEnvironment": "node",
        "testPathIgnorePatterns": [
          "/node_modules/",
          "/dist/",
          ".*\\.browser\\.js"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>enzyme.config.js",
        "globals": {
          "__DEV__": true,
          "__NODE__": true,
          "__BROWSER__": false
        }
      }
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-fusion"
    ]
  },
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.6.0",
    "yarn": ">=1.4.0"
  },
  "private": true,
  "files": [
    "dist",
    "src",
    "!__tests__"
  ]
}
