{
  "name": "@uber/graphene-logtron",
  "version": "0.2.3",
  "description": "Graphene plugin for universal logging via logtron",
  "main": "./dist/node.cjs.js",
  "module": "./dist/node.es.js",
  "browser": {
    "./dist/node.cjs.js": "./dist/browser.cjs.js",
    "./dist/node.es.js": "./dist/browser.es.js"
  },
  "es2015": {
    "./dist/node.cjs.js": "./dist/node.cjs.es2015.js",
    "./dist/node.es.js": "./dist/node.es.es2015.js",
    "./dist/browser.cjs.js": "./dist/browser.cjs.es2015.js",
    "./dist/browser.es.js": "./dist/browser.es.es2015.js"
  },
  "dependencies": {
    "@uber/babel-helpers": "^0.2.1",
    "@uber/logtron": "^9.0.5",
    "koa-bodyparser": "4.2.0",
    "error-stack-parser": "^2.0.1",
    "source-map": "^0.6.1"
  },
  "peerDependencies": {
    "@uber/graphene-app": "^0.2.1",
    "@uber/graphene-plugin": "^0.2.1"
  },
  "devDependencies": {
    "@uber/eslint-config-graphene": "^0.2.1",
    "@uber/graphene-universal-events": "^0.2.1",
    "@uber/graphene-app": "^0.2.1",
    "@uber/graphene-plugin": "^0.2.1",
    "create-universal-package": "^1.0.0-rc.18",
    "eslint": "^4.10.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-istanbul": "^4.1.5",
    "eslint-plugin-cup": "^1.0.0-rc.4",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "1.7.4",
    "nyc": "^11.2.1",
    "tape-cup": "^4.7.1",
    "unitest": "^1.1.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint src/",
    "transpile": "npm run clean && cup build",
    "build-test": "rm -rf dist-test && cup build-tests",
    "just-test": "unitest --browser=dist-tests/browser.js --node=dist-tests/node.js",
    "test": "npm run build-test && npm run just-test",
    "cover": "npm run build-test && npm run just-cover",
    "just-cover": "nyc --reporter=cobertura npm run just-test",
    "prepublish": "npm run transpile"
  },
  "engines": {
    "node": ">=8.3.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  }
}
